#include<iostream>
using namespace std;
int main()
{
    int a;
    cin>>a;
    int arr[a];//={1,2,3,4,5};
    for(int i=0;i<a;i++){
        cin>>arr[i];
    }
    int pos;
    cin>>pos;
    for(int i=pos;i<a;i++){
        arr[i]=arr[i+1];
    }
    for(int i=0;i<a-1;i++){
        cout<<arr[i];
    }
    return 0;
}
