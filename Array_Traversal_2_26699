#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <iomanip>
using namespace std;


int main() {
  
    double sum=0;
    int n=5;
    float arr[5];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    for(int i=0;i<n;i++){
        if(arr[i]>100){
            arr[i]-=((20*arr[i])/100);
            
          
        }
    
    }
    
    for(int i=0;i<n;i++){
        sum=sum+arr[i];
    }
    cout<<fixed<<setprecision(2)<<sum;
    
    return 0;
}
