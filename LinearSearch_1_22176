#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;




int main()
{
    int n;
    cin>>n;

    char arr[n];

    for(int i=0;i<n;i++)
    {
       cin>>arr[i];
    }
    
    int count=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]=='W')
        {
            cout<<i<<" ";
            count++;
        }
    }

    while(count==0)
    {
        cout<<-1;
        break;
    }
   cout<<endl<<count;


}
